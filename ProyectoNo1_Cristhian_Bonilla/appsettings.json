{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server={DB_SERVER},{DB_PORT};Database={DB_DATABASE};User Id={DB_USER};Password={DB_PASSWORD};Encrypt=False;TrustServerCertificate=True;MultipleActiveResultSets=true;"
  }
}
